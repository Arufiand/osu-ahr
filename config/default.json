{
  "log4js": {
    "configure": {
      "appenders": {
        "console": {
          "type": "logLevelFilter",
          "level": "INFO",
          "appender": {
            "type": "console"
          }
        },
        "file": {
          "type": "dateFile",
          "filename": "logs/test.log"
        }
      },
      "categories": {
        "default": {
          "appenders": [
            "console"
          ],
          "level": "debug"
        },
        "irc": {
          "appenders": [
            "console",
            "file"
          ],
          "level": "all"
        },
        "lobby": {
          "appenders": [
            "console",
            "file"
          ],
          "level": "all"
        },
        "plugins": {
          "appenders": [
            "console",
            "file"
          ],
          "level": "all"
        }
      }
    }
  },
  "trial": {
    "env": "default",
    "default_value": "default"
  },
  "irc": {
    "server": "irc.ppy.sh",
    "nick": "your account name",
    "opt": {
      "port": 6667,
      "password": "your Server Password",
      "_what is server password?": "access to https://osu.ppy.sh/p/irc"
    }
  },
  "HostSkipper": {
    "skip_request_rate": 0.5,
    "skip_request_min": 2,
    "skip_timer_delay_ms": 20000
  },
  "OahrCli": {
    "invite_users": [],
    "password": ""
  }
}